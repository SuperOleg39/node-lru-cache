{
  "name": "lru-cache",
  "description": "A cache object that deletes the least-recently-used items.",
  "version": "7.4.2",
  "author": "Isaac Z. Schlueter <i@izs.me>",
  "keywords": [
    "mru",
    "lru",
    "cache"
  ],
  "scripts": {
    "prepare": "rollup index.js --file bundle/main.js --format cjs",
    "build": "npm run prepare",
    "presize": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "size": "size-limit",
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags"
  },
  "main": "index.js",
  "repository": "git://github.com/isaacs/node-lru-cache.git",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "benchmark": "^2.1.4",
    "rollup": "^2.70.0",
    "size-limit": "^7.0.8",
    "tap": "^15.1.6"
  },
  "license": "ISC",
  "files": [
    "index.js",
    "bundle"
  ],
  "engines": {
    "node": ">=12"
  },
  "tap": {
    "coverage-map": "map.js"
  },
  "size-limit": [
    {
      "path": "./bundle/main.js"
    }
  ]
}
